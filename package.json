{
  "name": "bk-assesment",
  "version": "1.0.0",
  "description": "bk assesment backend",
  "main": "index.ts",
  "type": "module",
  "scripts": {
    "dev": "nodemon --exec node --loader ts-node/esm src/index.ts",
    "start": "tsc && node dist/index.js",
    "build": "tsc -p .",
    "test": "mocha ./tests/*.ts",
    "lint": "eslint . --ext .ts",
    "db:migrate": "cross-env NODE_OPTIONS='--require tsconfig-paths-bootstrap.js' sequelize db:migrate",
    "db:seed": "cross-env NODE_OPTIONS='--require tsconfig-paths-bootstrap.js' sequelize db:seed:all"
  },
  "author": "Landry",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^4.3.16",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.21",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.12.12",
    "@types/npmlog": "^7.0.0",
    "@types/sequelize": "^4.28.20",
    "@types/sinon": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "chai": "^5.1.1",
    "chai-http": "^4.4.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "mocha": "^10.4.0",
    "nodemon": "^3.0.3",
    "nyc": "^15.1.0",
    "prettier": "^3.2.4",
    "sequelize-cli": "^6.3.0",
    "sinon": "^18.0.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.2",
    "express": "^4.18.2",
    "express-form-data": "^2.0.23",
    "http": "^0.0.1-security",
    "npmlog": "^7.0.1",
    "pg": "^8.11.5",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.3",
    "sequelize-typescript": "^2.1.6"
  }
}
